{
    "name": "czapla/r_socket",
    "description": "library for rsocket",
    "type": "library",
    "license": "MIT License",
    "authors": [
        {
            "name": "czapla",
            "email": "poczta.czapla.pl@gmail.com"
        }
    ],
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "infection/extension-installer": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "require-dev": {
        "bmitch/churn-php": "^1.6",
        "clue/block-react": "1.5",
        "friendsofphp/php-cs-fixer": "^3.8",
        "infection/infection": "^0.26.6",
        "jetbrains/phpstorm-attributes": "^1.0",
        "phpmd/phpmd": "^2.12",
        "phpstan/phpstan": "^1.5",
        "phpunit/phpunit": "9.5.21",
        "vimeo/psalm": "^4.22"
    },
    "require": {
        "react/socket": "1.12.0"
    }
}
