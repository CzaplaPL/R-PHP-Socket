{
    "name": "czapla/r_php_socket",
    "description": "library for rsocket",
    "type": "library",
    "license": "MIT License",
    "authors": [
        {
            "name": "czapla",
            "email": "poczta.czapla.pl@gmail.com"
        }
    ],
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.8",
        "phpstan/phpstan": "^1.5",
        "phpunit/phpunit": "9",
        "vimeo/psalm": "^4.22"
    }
}
